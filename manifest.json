{
  "name": "Github XP",
  "version": "3.14",
  "description": "Github <3 Microsoft!",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["xp.css"],
      "scripts": ["xp.js"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
